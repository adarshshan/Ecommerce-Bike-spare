</head>

<body>
  <header>

    <!-- popup  -->
    <div class="popup shadow" id="popup">
      <img src="https://cdn.pixabay.com/photo/2014/04/02/11/01/tick-305245_640.png" alt="">
      <h2>Success</h2>
      <p id="message-alert">Password changed successfully !</p>
      <button class="bttn" type="button" onclick="closePopup()">Ok</button>
  </div>

    <nav class="navbar navbar-expand-lg navbar-light border-3 border-bottom py-3">
      <div class="container-fluid">
        <a class="navbar-brand fs-1 fw-bold ms-4 text-danger" href="/">Spare<span class="text-dark">KIT</span></a>
        <form class="d-flex ms-5">
          <input class="inp rounded-pill p-2" type="search" placeholder="Search">
          <button class="btn" type="submit"></button>
        </form>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">

          <ul class="navbar-nav ms-auto fs-5 mb-2 mb-lg-0">
            <li class="nav-item">
              <a class="nav-link active text-white" aria-current="page" href="/">Home</a>
            </li>

            <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle text-white" href="#" id="navbarDropdown" role="button"
                data-bs-toggle="dropdown" aria-expanded="false">
                Brand
              </a>
              <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                <li><a class="dropdown-item" href="#">Bajaj</a></li>
                <li><a class="dropdown-item" href="#">Yamaha</a></li>
                <li><a class="dropdown-item" href="#">Royal Enfield</a></li>
                <li><a class="dropdown-item" href="#">Honda</a></li>
              </ul>
            </li>

            <li class="nav-item dropdown me-5">
              <a class="nav-link dropdown-toggle text-white btn btn-primary" href="#" id="navbarDropdown" role="button"
                data-bs-toggle="dropdown" aria-expanded="false">
                Login
              </a>
              <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                <li><a class="dropdown-item" href="/profile"><i class="fa-solid fa-user"></i> My Profile</a></li>
                <li><a class="dropdown-item" href="#">Wish List</a></li>
                <li><a class="dropdown-item" href="#">24X7 Costomer care</a></li>
            </li>
          </ul>
          </li>
          </ul>

        </div>
      </div>
    </nav>
  </header>
<section class="vh-100" style="background-color: #f7efef;">
    <div class="container h-100">
      <div class="row d-flex justify-content-center align-items-center h-100">
        <div class="col-lg-12 col-xl-11">
          <div class="card text-black" style="border-radius: 25px;">
            <div class="card-body p-md-5">
              <div class="row justify-content-center">
                <div class="col-md-10 col-lg-6 col-xl-5 order-2 order-lg-1" id="inner">
  
                  <p class="text-center h1 fw-bold mb-5 mx-1 mx-md-4 mt-4">New password</p>
                  <div id="message"></div>
                    
  
                    <div class="d-flex flex-row align-items-center mb-4">
                      <i class="fas fa-envelope fa-lg me-3 fa-fw"></i>
                      <div class="form-outline flex-fill mb-0">
                        <label class="form-label" for="form3Example3c">Password</label>
                        <input name="email" type="password" id="password" class="form-control" />
                        <span class="text-danger" id="password-error"></span>
                      </div>
                    </div>

                    <div class="d-flex flex-row align-items-center mb-4">
                      <i class="fas fa-lock fa-lg me-3 fa-fw"></i>
                      <div class="form-outline flex-fill mb-0">
                        <label class="form-label" for="form3Example4c">confirm Password</label>
                        <input name="password" type="password" id="cpassword" class="form-control" />
                        <span class="text-danger" id="cpassword-error"></span>
                      </div>
                    </div>
                    
                    <button class="btn btn-primary p-3" onclick="newPass()">Save password</button>
                    <!-- <input type="submit" value="Login" class="btn btn-primary p-3"> -->
                  
                </div>
                <div class="col-md-10 col-lg-6 col-xl-7 d-flex align-items-center order-1 order-lg-2">
  
                  <img src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-registration/draw1.webp"
                    class="img-fluid" alt="Sample image">
  
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <script src="/static/user/common.js"></script>
